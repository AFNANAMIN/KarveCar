                    <!--Resource que permite hacer drag&drop entre los ribbongroups-->
                    <Style TargetType="r:RibbonGroup">
                        <Setter Property="AllowDrop" Value="True"/>
                        <EventSetter Event="PreviewMouseMove" Handler="RibbonGroup_PreviewMouseMove"/>
                        <EventSetter Event="Drop" Handler="RibbonGroup_Drop"/>
                    </Style>


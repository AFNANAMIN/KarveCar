                <RowDefinition Height="Auto"></RowDefinition>
                <RowDefinition Height="Auto"></RowDefinition>
                <RowDefinition Height="Auto"></RowDefinition>

            </Grid.RowDefinitions>
            <ContentControl ContentTemplate="{StaticResource Risk}" Grid.Row="0"></ContentControl>

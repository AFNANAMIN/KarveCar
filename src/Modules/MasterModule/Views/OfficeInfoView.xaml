                                               AllowNull="True"
                                                                    
                                                                      FormatString="t"
                                                                      
                                                                      AllowInlineEditing="True"
                                               InputScope="Number"  Value="{Binding HolidayTimeTo,Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                               MinWidth="200" Width="auto" HorizontalAlignment="Left">
                                            </syncfusion:SfTimePicker>

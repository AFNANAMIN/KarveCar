                                                     karveControls:ControlExt.Attach="True" 
                                                    
                                                     karveControls:ControlExt.DataSource="{Binding DataObject}"
                                                     karveControls:ControlExt.ItemChangedCommand="{Binding ItemChangedCommand}"
                                                     karveControls:ControlExt.Password="{Binding Path=DataObject.PWD_WEB, Mode=TwoWay, ValidatesOnDataErrors=True, ValidatesOnNotifyDataErrors=True, ValidatesOnExceptions=True}" 
                                                     Height="30" x:Name="pWDWEB_EMP" Margin="0,0,0,10"/>
                                        </Grid>
                                    </StackPanel>
                                </GroupBox>

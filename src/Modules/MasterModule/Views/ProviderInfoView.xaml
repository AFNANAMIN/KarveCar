                                        </i:EventTrigger>
                                        <i:EventTrigger EventName="DataSearchTextBoxChanged">
                                            <mvvm1:InvokeCommandAction Command="{Binding ItemChangedCommand, PresentationTraceSources.TraceLevel=High}" 

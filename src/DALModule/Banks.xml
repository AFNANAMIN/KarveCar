<?xml version="1.0" encoding="UTF-8" ?>
<sqlMap namespace="Banks" xmlns="http://ibatis.apache.org/mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" >	       
	<resultMaps>                                    
		<resultMap id="BankResultMap"  class="BankDataObject" >
			<result property="Code"     column="CODBAN"/>
			<result property="Name"    column="NOMBRE"/>
			<result property="Swift"     column="SWIFT"/>
		</resultMap>	    
	</resultMaps>
  <statements>
    <sql id="GetAllBanks">
      select 
      * 
      from 
      Banco 
      order by NOMBRE
    </sql>
</statements>
</sqlMap>